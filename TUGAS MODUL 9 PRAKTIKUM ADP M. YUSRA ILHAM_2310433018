import os
from termcolor import cprint, colored
os.system('cls') 
for i in range (2):
    cprint(" "*40,'white',"on_blue")
for j in range (1):
    cprint("   <<<         // ||  ////   |||||      ",'blue',"on_white")
    cprint("      \\\\      //  || ||     ||   ||     ",'blue',"on_white")
    cprint("       \\\\    //   ||  \\\\\\\\  |||||||     ",'blue',"on_white")
    cprint("        \\\\  //    ||     || ||   ||     ",'blue',"on_white")
    cprint("         \\\\//     ||  ////  ||   ||     ",'blue',"on_white")
for k in range (2):
    cprint(" "*40,'white',"on_yellow")
